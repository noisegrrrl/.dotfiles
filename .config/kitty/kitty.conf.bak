shell /usr/bin/fish

# BEGIN_KITTY_FONTS
font_family Maple Mono NF CN
# END_KITTY_FONTS

# BEGIN_KITTY_THEME
# Root-Loops
include current-theme.conf
# END_KITTY_THEME

shell_integration no-cursor
cursor_shape block

hide_window_decorations yes
tab_bar_style powerline

enable_audio_bell no

text_fg_override_threshold 7 ratio 
